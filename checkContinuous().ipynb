    "def checkContinuous(target):\n",
    "    continuous_count = 0\n",
    "    \n",
    "    for i in range(len(target) - 1):\n",
    "        if target[i] == target[i + 1]:\n",
    "            continuous_count += 1\n",
    "    return continuous_count"
   ]
